  },
  "files": [ "src/", "lib/", "examples/", "interception/", "dist/", "binding.gyp" ],
  "devDependencies": {
    "@types/node": "^16.4.13",
    "node-gyp": "^8.1.0",
    "ts-node": "^10.2.0",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "node-addon-api": "^4.0.0"
  }
